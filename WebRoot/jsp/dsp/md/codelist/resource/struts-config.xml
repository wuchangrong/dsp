<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">

<struts-config>

    <!-- ========== Form Bean Definitions =================================== -->
    <form-beans>
        <form-bean name="WMS_BASE_CODELIST_CodelistForm" type="com.handao.dsp.md.codelist.web.CodelistForm"/>        

    </form-beans>
    <!-- ========== End of Form Bean Definitions =================================== -->

    <!-- ========== Action Mapping Definitions ============================== -->
    <action-mappings>

	  <action name="WMS_BASE_CODELIST_CodelistForm" parameter="method" path="/dsp/md/codelist/Codelist" type="com.handao.dsp.md.codelist.web.CodelistAction">
            <forward name="list" path="/jsp/dsp/md/codelist/CodelistList.jsp" />
            <forward name="list.do" path="/dsp/md/codelist/Codelist.do?method=doSearch"/>
            <forward name="addIni.do" path="/dsp/md/codelist/Codelist.do?method=doAddIni&amp;doWhat=add"/>
            <forward name="addEdit" path="/jsp/dsp/md/codelist/CodelistAddEdit.jsp?method=doAddEditIni"/>
            <forward name="view" path="/jsp/dsp/md/codelist/CodelistView.jsp?method=doView"/>
        </action>
 

	  </action-mappings>
    <!-- ========== End of Action Mapping Definitions ============================== -->
	<message-resources key="dsp.md.codelist"  parameter="com.handao.dsp.md.codelist.web.MessageMapping"/>

</struts-config>